define servicedependency {
<% %W(dependent_host_name dependent_hostgroup_name dependent_service_description
      host_name hostgroup_name service_description inherits_parent
      execution_failure_criteria notification_failure_criteria dependency_period
   ).each do |key| %>
<% if cc.has_key?(key) && !cc[key].to_s.empty? %>
  <%= key %>    <%= cc[key].to_s %>
<% end %>
<% end %>
}
